	.section-img.section-img_floor
		.floor-block 
			.tooltip
				.tooltip__arrow.tooltip__arrow_top 
				.tooltip__text 
					.tooltip__top 
						.tooltip__house Дом 1
						.tooltip__floor Этаж 1
					.tooltip__row Свободно: 0 квартир
					.tooltip__row Куплено: 26  квартир
